@model VHSOnly.Models.Customer
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <il>Membership Type:@Model.MembershipType.Name</il>
    @if (Model.Birthday.HasValue)
    {
        <il>@Model.Birthday.Value.ToShortDateString()</il>
    }
</ul>



